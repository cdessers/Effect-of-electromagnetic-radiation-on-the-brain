/* This is a comment */
/* This is a true multiple line comment !
 * Yo !
 * Re-bonjour */
//This is another comment
$INFOS
	$NAME
		// Output file for fields:
		output=OUTPUT_FILES
		// Error log file:
		error=ERROR_LOG
		// Profiling (cpu time, memory, etc):
		profile=PROFILING
	$NAME
$INFOS

$MESH
	// Size of the elements of the mesh:
	$DELTAS
		deltaX_Electro=1.0
		deltaY_Electro=1.0
		deltaZ_Electro=1.0
		delta_Thermal=1.0
	$DELTAS
		
	// Boundaries of the domain
	$DOMAIN_SIZE
		// Length of the domain in each direction
		L_X=20
		L_Y=20
		L_Z=20
	$DOMAIN_SIZE
	
	// Information on the source
	$SOURCE
		NBR_SOURCES=1
		// Length of the source in each direction
		L_X=10
		L_Y=10
		L_Z=10
		// Center of the source, (0,0,0) is in the lower left corner in front:
		C_X=100
		C_Y=100
		C_Z=100
		// Frequency [Hz]
		FRQCY=900E6
		// Air gap (in meter)
		AIR_GAP=1;2
	$SOURCE
	
	$MATERIALS
		USE_AIR_EVERYWHERE=true
	$MATERIALS
	
$MESH

$RUN_INFOS
	$STOP_SIMUL_AFTER
	// The simulation will stop after... (in sec)
	stopTime=10
	$STOP_SIMUL_AFTER
	
	// Initial temperatures:
	$TEMP_INIT
		T_INIT_AIR   =25
		T_INIT_WATER =29
	$TEMP_INIT
	
$RUN_INFOS
	